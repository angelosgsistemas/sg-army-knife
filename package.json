{
	"name": "sg-army-knife",
	"displayName": "SG Army Knife",
	"icon": "images/sg.ico",
	"description": "Conjunto de utilidades para VS Code da SG Sistemas.",
	"publisher": "WesleyNepomuceno",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sg-army-knife.exportaArquivo",
		"onCommand:sg-army-knife.exportaTodosArquivos",
		"onCommand:sg-army-knife.compilador",
		"onCommand:sg-army-knife.pegaindices"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "SGArmyKnife",
			"properties": {
				"Caminho servidor Harbour-Ubuntu": {
					"type": "string",
					"default": "\\192.168.1.232\\usuario\\trunk",
					"description": "Caminho do repositório na raiz."
				},
				"Caminho servidor xHarbour": {
					"type": "string",
					"default": "\\192.168.1.230\\usuario\\trunk",
					"description": "Caminho do repositório na raiz."
				},
				"Caminho servidor Harbourx64": {
					"type": "string",
					"default": "\\192.168.1.231\\usuario\\trunk",
					"description": "Caminho do repositório na raiz."
				},
				"Caminho servidor Harbourx86": {
					"type": "string",
					"default": "\\192.168.1.233\\usuario\\trunk",
					"description": "Caminho do repositório na raiz."
				},
				"Caminho servidor Extra": {
					"type": "string",
					"default": "\\192.168.1.222\\usuario\\trunk",
					"description": "Caminho do repositório na raiz."
				},

				"Repositório": {
					"type": "string",
					"default": "T:",
					"description": "Caminho do repositório local na new."
				}
			}
		},
		"commands": [
			{
				"command": "sg-army-knife.exportaArquivo",
				"title": "SG: Exporta arquivo atual"
			},
			{
				"command": "sg-army-knife.exportaTodosArquivos",
				"title": "SG: Exporta todos arquivos da branch atual"
			},
			{
				"command": "sg-army-knife.compilador",
				"title": "SG: Compilador"
			},
			{
				"command": "sg-army-knife.pegaindices",
				"title": "SG: Pega Indice"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"esbuild": "^0.14.9",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
